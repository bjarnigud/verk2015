@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>SozialWeb</h1>
</div>
<div class="row">
    <div class="col-md-4">
        <h2>Welcome to SozialWeb</h2>
    </div>
</div>

@foreach (var item in ViewBag.Posts)
{
    <blockquote>
        <p>@item.text</p>
        <footer>@item.author.Name - @item.timeOfPost.ToString("dd.MM.yyyy HH:mm")</footer>
    </blockquote>
}